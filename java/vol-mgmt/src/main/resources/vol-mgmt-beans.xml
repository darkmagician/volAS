<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation="
      http://www.springframework.org/schema/beans
      http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
      http://www.springframework.org/schema/context
      http://www.springframework.org/schema/context/spring-context-3.2.xsd">
    <bean id="MVELPromotionPolicy"
     class="com.vol.mgmt.MVELPromotionPolicy"/>
     
   <bean id="promotionMgmt"
     class="com.vol.mgmt.PromotionMgmtImpl" init-method="init"/>
 
   <bean id="operatorMgmt"
     class="com.vol.mgmt.OperatorMgmtImpl" init-method="init"/>
     
   <bean id="tenantMgmt"
     class="com.vol.mgmt.TenantMgmtImpl" init-method="init"/>     
 
    <bean id="cycleHandler"
     class="com.vol.mgmt.CycleHandler"/> 
     
      
    <bean id="bonusMgmt"
     class="com.vol.mgmt.BonusMgmtImpl" init-method="init"/>    

    <bean id="quotaMgmt"
     class="com.vol.mgmt.QuotaMgmtImpl" init-method="init"/>       
     
    <bean id="userMgmt"
     class="com.vol.mgmt.UserMgmtImpl" init-method="init"/>            
</beans>